<?xml version="1.0" encoding="UTF-8" ?>

<!-- This file contains the standard speech said by SU for each service provided. -->

<su_standard_speech xmlns="">
    <pay_attention>
        <fr>oui, </fr>
        <en>yes, </en>
    </pay_attention>
    <agree>
        <fr>Oui</fr>
        <en>Yes</en>
    </agree>
    <understood>
        <fr>Ok</fr>
        <en>Ok</en>
    </understood>
    <greeting>
        <fr>
            <start>Bonjour</start>
            <end>je suis à vôtre disposition</end>
        </fr>
        <en>
            <start>Hello</start>
            <end>I'm available for your needs</end>
        </en>
    </greeting>
    <take_photo>
        <before>
            <fr>Soyez prêt, je vous prend en photo</fr>
            <en>Be ready, I will take a photo</en>
        </before>
        <after>
            <fr>ça y est !</fr>
            <en>I'm done !</en>
        </after>
    </take_photo>
    <take_screenshot>
        <before>
            <fr>Ok, je capture l'intégralité de l'écran</fr>
            <en>Okay, I'm taking a screenshot</en>
        </before>
        <after>
            <fr>ça y est ! Vous pouvez l'enregistrer en faisant control s</fr>
            <en>I'm done! You can save it by doing control s</en>
        </after>
    </take_screenshot>
    <record_video>
        <before>
            <fr>Ok, je patiente quelques secondes. Appuyez la touche q pour terminer.</fr>
            <en>I'm filming, touch q to finish.</en>
        </before>
        <after>
            <fr>c'est dans la boîte !</fr>
            <en>It's in the box !</en>
        </after>
    </record_video>
    <tell_time>
        <before>
            <fr>Il est </fr>
            <en>It's </en>
        </before>
        <between>
            <fr>et </fr>
            <en>and </en>
        </between>
        <after>
            <hours>
                <fr>heures </fr>
                <en>hours </en>
            </hours>
            <minutes>
                <fr>minutes</fr>
                <en>minutes</en>
            </minutes>
        </after>
    </tell_time>
    <tell_date>
        <fr>Nous sommes le</fr>
        <en>Today's date is</en>
    </tell_date>
    <look_up>
        <keyword>
            <fr>cherche</fr>
            <en>look up</en>
        </keyword>
        <fr>Voici les résultats obtenus sur le web</fr>
        <en>Hera are the results gathered on the web</en>
    </look_up>
    <open>
        <before>
            <fr>J'ouvre</fr>
            <en>Opening</en>
        </before>
        <after>
            <browser>
                <fr>le navigateur</fr>
                <en>the browser</en>
            </browser>
        </after>
    </open>
</su_standard_speech>
